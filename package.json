{
  "name": "@sabinthedev/nestjs-prisma",
  "version": "1.0.50",
  "description": "A NestJS module that allows you use Prisma, set up multiple Prisma services, and use multi-tenancy in each Prisma service.",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --check --plugin-search-dir=. \"**/*.{ts,tsx}\"",
    "format:fix": "prettier --write --plugin-search-dir=. \"**/*.{ts,tsx}\"",
    "test": "vitest",
    "test:output": "vitest --reporter verbose",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "keywords": [
    "nestjs",
    "prisma",
    "module",
    "plugin",
    "database",
    "multi-tenancy",
    "typescript"
  ],
  "author": "Sabin Adams",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.7.0",
    "@nestjs/common": "^10.3.10",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/testing": "^10.3.10",
    "@types/node-fetch": "2.6.11",
    "@types/sinon": "17.0.3",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitest/ui": "^2.0.4",
    "connection-string": "*",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "globals": "^15.8.0",
    "husky": "^9.1.2",
    "lint-staged": "15.2.7",
    "prettier": "3.3.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sinon": "18.0.0",
    "typescript": "5.5.4",
    "vitest": "^2.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "connection-string": "^4.4.0"
  }
}
